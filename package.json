{
    "name": "check",
    "version": "0.0.1",
    "description": "Check payment",
    "main": "dist/index",
    "scripts": {
        "serve": "npx webpack serve --config ./configs/webpack.config.js",
        "build": "npm run build:ts && npm run build:files",
        "build:ts": "npm run remove:dist && npx tsc --build tsconfig.release.json",
        "build:files": "cp package.json package-lock.json README.md dist/",
        "test": "npx jest --config ./configs/jest.config.js",
        "test:watch": "npm run test -- --watchAll",
        "test:coverage": "npx jest --coverage",
        "lint": "npx eslint --ext .ts --ext .tsx src/",
        "lint:fix": "npm run lint -- --fix",
        "publish": "npm run build && npm publish dist/ --public",
        "clean": "npm run remove:dist && npm run remove:coverage",
        "remove:dist": "rm -rf dist",
        "remove:coverage": "rm -rf coverage",
        "revert": "rm -rf node_modules && rm -rf package-lock.json",
        "reinstall": "npm run revert && npm i"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.5",
        "@babel/preset-typescript": "^7.12.1",
        "@types/jest": "^26.0.15",
        "@types/react": "^16.9.55",
        "@types/react-dom": "^16.9.9",
        "@types/react-redux": "^7.1.11",
        "@types/redux-actions": "^2.6.1",
        "@types/redux-immutable-state-invariant": "^2.1.1",
        "@types/stylus": "^0.48.33",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.6.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.1",
        "babel-plugin-stylus-compiler": "^1.4.0",
        "css-loader": "^1.0.1",
        "eslint": "^7.12.1",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-airbnb-typescript": "^12.0.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jasmine": "^4.1.1",
        "eslint-plugin-jest": "^24.1.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-lodash": "^7.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-sonarjs": "^0.5.0",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^26.6.3",
        "postcss": "^8.1.10",
        "prettier": "2.1.2",
        "prop-types": "^15.7.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-redux": "^7.2.2",
        "redux-actions": "^2.6.5",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-observable": "^1.2.0",
        "style-loader": "^2.0.0",
        "stylus": "^0.54.8",
        "stylus-loader": "^4.3.0",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.5",
        "webpack": "^5.4.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.4.0"
    },
    "dependencies": {
        "axios": "^0.21.0"
    }
}
